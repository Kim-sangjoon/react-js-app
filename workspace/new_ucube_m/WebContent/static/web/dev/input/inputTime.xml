								<w2:attributes>
									<w2:scope>row</w2:scope>
								</w2:attributes>
								<w2:textbox tagname="label" style="" id="" label="시간(udc)" class="req"></w2:textbox>
							</xf:group>
							<xf:group tagname="td" style="" class="w2tb_td">
								<w2:attributes>
									<w2:colspan>1</w2:colspan>
									<w2:rowspan>1</w2:rowspan>
								</w2:attributes>
