						<w2:key id="deptCd" name="부서코드" dataType="text"></w2:key>
					</w2:keyInfo>
					<w2:data></w2:data>
				</w2:dataMap>
			</w2:dataCollection>
			<w2:workflowCollection></w2:workflowCollection>
		</xf:model>
		<script type="text/javascript" lazy="false"><![CDATA[
	scwin.onpageload = function() {
		/*
			시간컴포넌트는 udc 탭에서 생성합니다.
		*/
	};

	scwin.onpageunload = function() {

	};
]]></script>
		<w2:require src="/common/udc/udc_timePicker.xml"></w2:require>
	</head>
	<body ev:onpageload="scwin.onpageload" ev:onpageunload="scwin.onpageunload">
		<xf:group id="" class="sub_wrapper">
			<w2:wframe src="../../../common/xml/contentsTitle.xml" scope="true" style="" id="" class="page_header" nextTabID=""></w2:wframe>
			<xf:group id="" class="page_contents">
			<xf:group id="" class="page_contents_inner"><xf:group style="" id="tbl_userInfo" class="tbbox">
					<xf:group tagname="table" style="width:100%;" id="" class="w2tb tb">
						<w2:attributes>
							<w2:summary></w2:summary>
						</w2:attributes>
						<xf:group tagname="caption"></xf:group>
						<xf:group tagname="colgroup">
							<xf:group tagname="col" style="width:15.00%;"></xf:group>
							<xf:group tagname="col" style=""></xf:group>
						</xf:group>
						<xf:group tagname="tr" style="">
