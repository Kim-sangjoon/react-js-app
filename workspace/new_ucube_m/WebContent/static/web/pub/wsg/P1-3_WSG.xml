         <script type="text/javascript" lazy="false"><![CDATA[

	scwin.onpageload = function() {
		scwin.pageSet(1,15);
		tbx_total.setValue(dlt_grdAllData.getRowCount());
	};

	scwin.onpageunload = function() {

	};

	//로우수 변경시 서비스
	scwin.sbx_select11_onviewchange = function(info) {
		scwin.pageSet(1,info.newValue);
	};


	//임시 : 조회 서비스 구현시 삭제
	scwin.pageSet = function(page,value) {

		WebSquare.layer.showProcessMessage("message");
		setTimeout(function() {
		sbx_select11.setValue(value);
		var totalRow = dlt_grdAllData.getRowCount();

		var pageCnt = Math.ceil(totalRow /value);
		if(value=="0"){
			dlt_grdAllData.setJSON(dlt_grdAllData.getAllJSON());
		}else{
			var stIdx = value * (page-1)
			var edIdx = value * page-1;
			dlt_grdAllData.setJSON(dlt_grdAllData.getRangeJSON(stIdx,edIdx));
		}
		pgl_checkSelect.setCount(pageCnt,true);
		WebSquare.layer.hideProcessMessage();

		}, 500);


	};


	//임시 : 인덱스 페이징 처리 서비스 구현시 삭제
	scwin.pgl_checkSelect_onviewchange = function(info) {

		var newIdx = info.newSelectedIndex;
		var stIdx = sbx_select11.getValue() * (newIdx-1)
		var edIdx = (sbx_select11.getValue())* newIdx-1;
		scwin.pageSet(newIdx,sbx_select11.getValue())
	};


	//[추가] 버튼을 Click을 하면 데이터를 입력할 수 있는 팝업 창이 오픈 됨 (인덱스 페이징)
	scwin.btn_insertRow_onclick = function(e) {
		var url = "/pub/dev/wsg/insertPopup.xml"
		var data = { "type" :"insert" ,"callbackFn" : "scwin.popupCallBack" };
		var options = { id : "insertPopup",
						popupName : "개인정보",
						modal : true,
						width : 802,
						height: 345,
						type : "wframePopup"
						};
		 com.win.openPopup(url, options, data);
	};

	scwin.grd_indexPage_oncelldblclick = function(row,col) {
		var url = "/pub/dev/wsg/insertPopup.xml"
		var data = { "type" :"insert" ,"callbackFn" : "scwin.popupCallBack" };
		var options = { id : "insertPopup",
						popupName : "개인정보",
						modal : true,
						width : 802,
						height: 345,
						type : "wframePopup"
						};
		 com.win.openPopup(url, options, data);
	};


	//삭제
	scwin.btn_delteteData_onclick = function(e) {
		com.win.confirm("삭제하시겠습니까?", "scwin.deleteData");
	};


	//CheckBox 클릭 시 해당 Row가 삭제 상태로 바뀜. 다시 클릭하면 삭제 해제됨
	scwin.grd_indexPage_oncellclick = function(row,col) {
		if(col == 0){
			scwin.deleteRows([row]);
		}
	};

	//취소버튼 클릭시 전체 데이터 롤백이벤트
	scwin.btn_undoAll_onclick = function(e) {
		dlt_grdAllData.clearFilter();
		com.data.undoAll(dlt_grdAllData);
		ibx_filtertInput.setValue("");

		filterInfo = {
			pglId  : pgl_checkSelect, //페이징 컴포넌트
			pglFlag : false, //페이징 컴포넌트가 현재 선택한 페이지를 유지 여부 기본 false pglId값이 존재해야함
			rowSize : 15, // 한페이지당 보여줄 row 수 pglId값이 존재해야함
		}
		com.data.filterPaging(dlt_grdAllData,filterInfo);
	};

	scwin.ibx_filtertInput_oneditkeyup = function(info) {
		filterInfo = {
			pglId  : pgl_checkSelect, //페이징 컴포넌트
			pglFlag : false, //페이징 컴포넌트가 현재 선택한 페이지를 유지 여부 기본 false pglId값이 존재해야함
			rowSize : 15, // 한페이지당 보여줄 row 수 pglId값이 존재해야함
			filterVal  : info.newValue, // 필터 벨류
			//filterCol : "userNm,empId", //추가 필터시 필터 적용컬럼명 , 없는경우 전체컬럼을 대상으로 함
		}

		var  filterCnt = com.data.filterPaging(dlt_grdAllData,filterInfo);
		tbx_filterCnt.setValue(filterCnt);
	};


	scwin.btn_saveConfirm_onclick = function(e) {
		com.win.confirm("저장하시겠습니까?", "scwin.saveData");
	};

	scwin.saveData = function(e) {
		//저장 crud 작성예정
		com.sbm.execute(sbm_save);
	};


]]></script>
