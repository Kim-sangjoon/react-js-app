	// 아코디언
	scwin.btn_acd_onclick = function() {
		var obj = $('#'+this.id).parent().parent().parent();
		
		if(!obj.hasClass('active')) {
			obj.siblings().removeClass('active');
			obj.addClass('active');
		} else {
			obj.removeClass('active');
		}
	};]]></script>
